import random



def printBoard():
    board = []
    for i in range(1):
      row = []
      for j in range(3):
        row.append('-')
        board.append(row)
    for row in board:
      for item in row:
        print(item, end = " ")
      print()
    

def check_mark(row, col):
  for row in board:
    for col in row:
      if col == '-':
        return True
      else:
          return False
      

def main():
  print("test board")
  printBoard()
  print(check_mark(1,3))
  
  

main()
